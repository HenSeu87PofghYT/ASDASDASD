    local Tabs = {
        Main = Window:AddTab({ Title = "TELEPORT WORLD", Icon = "map-pin" }),
    }

    Tabs.Main:AddButton({
        Title = "TELEPORT TO WORLD 1",
        Description = "Teleport to the World",
        Callback = function()
            Window:Dialog({
                Title = "Continue to Teleport?",
                Content = "This will teleport you to the World 1.",
                Buttons = {
                    {
                        Title = "Confirm",
                        Callback = function()
                            local args = {
                                [1] = {
                                    [1] = {
                                        [1] = "\3",
                                        [2] = "Teleport",
                                        [3] = "To",
                                        [4] = 1
                                    }
                                }
                            }
                            
                            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
                            print("Confirmed the dialog.")
                        end
                    },
                    {
                        Title = "Cancel",
                        Callback = function()
                            print("Cancelled the dialog.")
                        end
                    }
                }
            })
        end
    })

    Tabs.Main:AddButton({
