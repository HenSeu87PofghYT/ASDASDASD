    local function checkToggle()
        while Toggle.Value do
            local args = {
                [1] = {
                    [1] = {
                        [1] = "\3",
                        [2] = "Click",
                        [3] = "Execute"
                    }
                }
            }
            
            game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))            
            print("Toggle changed:", Options.MyToggle.Value)
            wait()
        end
    end

    Toggle:OnChanged(checkToggle)
    checkToggle() 

